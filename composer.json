{
  "name": "enea/laravel-sequenceable",
  "description": "Facilitates the generation and autocompletion of a sequential value in the database",
  "keywords": [
    "sequence",
    "sequenceable",
    "succession",
    "autoincrement",
    "increment",
    "enea"
  ],
  "authors": [
    {
      "name": "enea dhack",
      "email": "enea.so@live.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.21|^v11.15",
    "vaened/php-sequence-generator": "^2.0.0"
  },
  "license": "MIT",
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^10.4",
    "orchestra/testbench": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Enea\\Sequenceable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enea\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Enea\\Sequenceable\\SequenceableServiceProvider"
      ]
    }
  }
}
