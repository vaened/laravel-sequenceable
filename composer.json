{
  "name": "enea/laravel-sequenceable",
  "description": "Facilitates the generation and autocompletion of a sequential value in the database",
  "keywords": [
    "sequence",
    "sequenceable",
    "succession",
    "autoincrement",
    "increment",
    "enea"
  ],
  "authors": [
    {
      "name": "enea dhack",
      "email": "enea.so@live.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0 || ^10",
    "illuminate/database": "^9.0 || ^10",
    "vaened/php-sequence-generator": "^1.0.0"
  },
  "license": "MIT",
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^9.5.10 || ^10.1",
    "orchestra/testbench": "^7.0 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Enea\\Sequenceable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enea\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Enea\\Sequenceable\\SequenceableServiceProvider"
      ]
    }
  }
}
